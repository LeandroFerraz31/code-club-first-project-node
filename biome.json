{
	"$schema": "https://biomejs.dev/schemas/1.8.1/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	
	"javascript":{
		"formatter":{
			"bracketSpacing": false,
			"quoteStyle":"single"
		}
	},
	"css":{
		"formatter":{
            "quoteStyle":"single"
        }
	}
	
}
